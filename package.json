{
	"name": "skillcamp-bots",
	"version": "1.2.0",
	"description": "The bots for SkillCamp Discord servers",
	"main": "index.js",
	"scripts": {
		"startdev": "node skillcamp.js && node skillcoin.js && node campmaster.js",
		"startpm2": "pm2 start ecosystem.config.js --update-env --scripts-prepend-node-path",
		"start-pc": "pm2 start ecosystem.config.js --update-env",
		"restartpm2": "pm2 restart ecosystem.config.js --update-env --scripts-prepend-node-path",
		"restart-pc": "pm2 restart ecosystem.config.js --update-env",
		"endpm2": "pm2 stop ecosystem.config.js --scripts-prepend-node-path",
		"end-pc": "pm2 stop ecosystem.config.js",
		"skillcamp": "node skillcamp.js",
		"skillcoin": "node skillcoin.js",
		"campmaster": "node campmaster.js",
		"pm2-delete": "pm2 delete all",
		"pm2-save": "pm2 save",
		"update": "npm update && npm audit fix && npm fund && npm outdated",
		"register-all": "node campmaster-register.js && node skillcamp-register.js && node skillcoin-register.js",
		"test": "jest",
		"clear": "node ./clear.js",
		"lint": "eslint ."
	},
	"author": "Joel Sprouse",
	"keywords": [
		"discord",
		"discord.js",
		"discord bot",
		"discord.js bot",
		"custom bot"
	],
	"dependencies": {
		"@discordjs/rest": "^1.3.0",
		"@google-cloud/local-auth": "^2.1.0",
		"@pm2/io": "^5.0.0",
		"db": "^3.4.0",
		"discord-api-types": "^0.37.28",
		"discord-xp": "^1.1.16",
		"discord.js": "^14.7.1",
		"dotenv": "^16.0.3",
		"format-json-files": "^1.0.6",
		"googleapis": "^105.0.0",
		"mysql2": "^3.1.1",
		"nodemailer": "^6.9.1",
		"pm2": "^5.2.2",
		"sequelize": "^6.28.0",
		"sqlite3": "^5.1.4",
		"winston": "^3.8.2",
		"winston-daily-rotate-file": "^4.7.1",
		"xmlhttprequest": "^1.8.0"
	},
	"devDependencies": {
		"eslint": "^8.32.0",
		"jest": "^29.1.2",
		"nodemon": "^2.0.20"
	}
}